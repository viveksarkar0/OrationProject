# Database - Use PostgreSQL for production (Vercel Postgres, Supabase, etc.)
DATABASE_URL="postgresql://username:password@host:port/database?sslmode=require"

# NextAuth - Set to your production domain for deployment
NEXTAUTH_URL="https://your-app-name.vercel.app"
NEXTAUTH_SECRET="your-secret-key-here-generate-with-openssl-rand-base64-32"

# Google OAuth - Get from Google Cloud Console
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"

# Gemini AI - Get from Google AI Studio
GEMINI_API_KEY="your-gemini-api-key"

# Vercel Environment (automatically set by Vercel)
VERCEL_ENV="production"
